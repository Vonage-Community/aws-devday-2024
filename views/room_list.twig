{% extends '_layouts/default.twig' %}

{% block content %}
<h1 class="text-3xl font-bold">Available Rooms</h1>

<ul>
{% for room in rooms %}
    <li><a href="/room/{{ room }}">{{ room }}</a></li>
{% endfor %}
</ul>

<a href="/generate-room">
    <vwc-button appearance="filled" label="Create New Room"></vwc-button>
</a>
{% endblock %}